#include<stdio.h>
#define bool int
bool isPowerOfFour(int n)
{
    if(n==0)
        return 0;
    while(n!=1)
    {
        if(n%4!=0)
            return 0;
        n=n/4;
    }
    return 1;
}
int main()
{
    int no=64;
    if(isPowerOfFour(no))
        printf("%d is a power of 4",no);
    else
       printf("%d is not a power of 4",no);
    getchar();
}
